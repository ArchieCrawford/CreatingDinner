import java.util.Scanner;

public class HelloWorld{

    public static void main(String[] args){

        System.out.println("Create a perfect meal for Dinner");
        // String
        String name, fruit, meat, drink, vegetable;

        System.out.println("Enter your Name: ");
        Scanner input = new Scanner(System.in);
        name = input.next();
        System.out.println("Vegetable will be:");
        vegetable = input.next();
        System.out.println("Fruit will be: ");
        fruit = input.next();
        System.out.println("Meat will be: ");
        meat = input.next();
        System.out.println("Drink will be: ");
        drink = input.next();

        System.out.println("For Dinner " + name + " will have " + meat + " as his meat, " + fruit + " and " + vegetable + " as his fruits and vegetables, and " + drink + " as has Drink.");

        // Array
        int food[]=new int[3];
        food [0] = 33 ;
        food [1] = 34  ;

        System.out.println(food[1]);



    }
}
 
